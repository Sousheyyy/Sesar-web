# Database
DATABASE_URL="your-database-url"
DIRECT_URL="your-direct-url"

# TikTok OAuth (Official API - Legacy, not used)
# TIKTOK_CLIENT_KEY="your-tiktok-client-key"
# TIKTOK_CLIENT_SECRET="your-tiktok-client-secret"
# TIKTOK_REDIRECT_URI="https://your-domain.com/api/auth/tiktok/callback"

# InsightIQ API Configuration (Primary TikTok Integration)
# Get your credentials from https://app.insightiq.ai
# Authentication: Basic Auth with Base64(Client_ID:Secret)
INSIGHTIQ_CLIENT_ID="your-insightiq-client-id"
INSIGHTIQ_CLIENT_SECRET="your-insightiq-client-secret"

# InsightIQ API Base URL (choose one based on your needs)
# Sandbox:    https://api.sandbox.insightiq.ai  (mock data for basic testing)
# Staging:    https://api.staging.insightiq.ai  (real connections with limitations)
# Production: https://api.insightiq.ai          (live data)
INSIGHTIQ_BASE_URL="https://api.staging.insightiq.ai"

# OAuth Redirect URI for InsightIQ
INSIGHTIQ_REDIRECT_URI="https://your-domain.com/api/auth/insightiq/callback"

# Encryption Key for storing tokens securely (64-character hex string = 32 bytes)
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
ENCRYPTION_KEY="your-64-character-hex-encryption-key-generate-with-crypto"

# NextAuth Configuration
NEXTAUTH_URL="https://your-domain.com"
NEXTAUTH_SECRET="your-nextauth-secret"

# Other environment variables
# Add your other environment variables here
